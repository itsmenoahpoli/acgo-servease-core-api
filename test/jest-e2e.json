{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/../src/$1",
    "^@/common/(.*)$": "<rootDir>/../src/common/$1",
    "^@/entities/(.*)$": "<rootDir>/../src/entities/$1",
    "^@/auth/(.*)$": "<rootDir>/../src/auth/$1",
    "^@/admin/(.*)$": "<rootDir>/../src/admin/$1",
    "^@/kyc/(.*)$": "<rootDir>/../src/kyc/$1",
    "^@/services/(.*)$": "<rootDir>/../src/services/$1",
    "^@/bookings/(.*)$": "<rootDir>/../src/bookings/$1",
    "^@/payments/(.*)$": "<rootDir>/../src/payments/$1",
    "^@/notifications/(.*)$": "<rootDir>/../src/notifications/$1",
    "^@/tenants/(.*)$": "<rootDir>/../src/tenants/$1",
    "^@/cities/(.*)$": "<rootDir>/../src/cities/$1",
    "^@/users/(.*)$": "<rootDir>/../src/users/$1",
    "^@/roles/(.*)$": "<rootDir>/../src/roles/$1",
    "^@/permissions/(.*)$": "<rootDir>/../src/permissions/$1"
  }
}
